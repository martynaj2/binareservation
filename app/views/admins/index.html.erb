<h3 class="text-center all-users-admin"> All users </h3>

<div class="container">
	<div class="users">
		<% @users.each do |user| %>
			<%= render 'users', user: user %>
		<%end%>
	</div>
</div>


<h3 class="text-center not-verified"> Not Verified: </h3>
<div class="container">
	<% @not_verified_users.each do |user| %>
		<%= render 'not_verified', user: user %>
	<% end %>
</div>
