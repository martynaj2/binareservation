	<div data-user-id="<%= user.id %>" class="row">
		 <div class="col-sm-12 col-md-9">
				<%= user.fullname %>
				<%= user.email %>
			</div>
			<div class="col-md-3 user-buttons">
				<%= link_to "Verify", verify_admin_path(id: user.id), method: :put, class:'btn btn-outline-success btn-customize' %>
				<%= link_to "Destroy", admin_path(user), method: :delete, remote: true, data: { confirm: "You sure?" }, class:'btn btn-outline-success btn-customize' %>
			</div>
	</div>
